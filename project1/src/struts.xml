<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
<constant name="struts.devMode" value="true" />

<package name="com.internousdev.project1.action" extends="struts-default">

<action name="LoginPageAction" class="com.internousdev.project1.action.LoginPageAction" method="execute">
	<result name="success">login.jsp</result>
</action>

<action name="UserCreatePageAction" class="com.internousdev.project1.action.UserCreatePageAction" method="execute">
	<result name="success">userCreate.jsp</result>
</action>

<action name="UserCreateConfirmAction" class="com.internousdev.project1.action.UserCreateConfirmAction" method="execute">
	<result name="success">userCreateConfirm.jsp</result>
	<result name="error">userCreate.jsp</result>
</action>

<action name="UserCreateCompleteAction" class="com.internousdev.project1.action.UserCreateCompleteAction" method="execute">
	<result name="success">userCreateComplete.jsp</result>
</action>

<action name="LoginAction" class="com.internousdev.project1.action.LoginAction" method="execute">
	<result name="success">home.jsp</result>
	<result name="error">login.jsp</result>
</action>

<action name="MyPageAction" class="com.internousdev.project1.action.MyPageAction" method="execute">
	<result name="success">myPage.jsp</result>
</action>

<action name="LogoutAction" class="com.internousdev.project1.action.LogoutAction" method="execute">
	<result name="success">home.jsp</result>
</action>

</package>

</struts>